server {
    listen       80;
    listen       [::]:80;
    server_name  {{ nginx_server_name }};
    root         /usr/share/nginx/html;

    location / {
    }

    error_page 404 /404.html;
        location = /40x.html {
    }

    error_page 500 502 503 504 /50x.html;
        location = /50x.html {
    }
}
